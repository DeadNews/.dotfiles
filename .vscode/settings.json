{
  "[json][jsonc][yaml][markdown][javascript][css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json][jsonc][yaml][markdown][toml][dockerfile]": {
    "editor.formatOnSave": true
  },
  "coverage-gutters.coverageBaseDir": ".",
  "diffEditor.renderSideBySide": false,
  "docker.images.description": ["Size", "CreatedTime"],
  "editor.cursorBlinking": "phase",
  "editor.fontFamily": "Fira Code Retina",
  "editor.fontLigatures": true,
  "editor.fontSize": 16,
  "editor.guides.bracketPairs": true,
  "editor.guides.bracketPairsHorizontal": false,
  "editor.renderWhitespace": "boundary",
  "editor.stickyScroll.maxLineCount": 1,
  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "scope": "log.error",
        "settings": {
          "foreground": "#e06c75"
        }
      },
      {
        "scope": "log.warning",
        "settings": {
          "foreground": "#e06c75"
        }
      },
      {
        "scope": "log.info",
        "settings": {
          "foreground": "#61afef"
        }
      }
    ]
  },
  "editor.wordWrap": "on",
  "explorer.confirmDragAndDrop": false,
  "files.associations": {
    "*.desktop": "properties",
    "*.json": "jsonc",
    "*.service": "properties"
  },
  "files.autoSave": "afterDelay",
  "files.trimTrailingWhitespace": true,
  "git.allowForcePush": true,
  "git.autofetch": true,
  "git.confirmSync": false,
  "git.enableSmartCommit": true,
  "github.copilot.enable": {
    "markdown": "true"
  },
  "githubPullRequests.defaultMergeMethod": "squash",
  "githubPullRequests.notifications": "pullRequests",
  "gitlens.codeLens.enabled": false,
  "gitlens.currentLine.enabled": false,
  "gitlens.hovers.enabled": false,
  "gitlens.statusBar.enabled": false,
  "inlineChat.holdToSpeech": false,
  "oneDarkPro.bold": true,
  "projectManager.git.baseFolders": ["~/git/"],
  "pyright.disableLanguageServices": false,
  "pyright.disableOrganizeImports": true,
  "python.languageServer": "None",
  "python.venvPath": "~/.cache/pypoetry/virtualenvs",
  "security.workspace.trust.enabled": false,
  "telemetry.telemetryLevel": "off",
  "terminal.integrated.copyOnSelection": true,
  "terminal.integrated.cursorStyle": "line",
  "terminal.integrated.fontFamily": "FiraCode Nerd Font Mono",
  "terminal.integrated.fontSize": 16,
  "terminal.integrated.persistentSessionReviveProcess": "never",
  "terminal.integrated.rightClickBehavior": "selectWord",
  "terminal.integrated.scrollback": 3000,
  "terminal.integrated.smoothScrolling": true,
  "terminal.integrated.stickyScroll.enabled": true,
  "update.mode": "none",
  "vsicons.dontShowNewVersionMessage": true,
  "window.newWindowDimensions": "maximized",
  "window.titleBarStyle": "custom",
  "workbench.colorTheme": "One Dark Pro",
  "workbench.iconTheme": "vscode-icons",
  "workbench.startupEditor": "none"
}
